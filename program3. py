a=int(input("ENTRE FIRST NUMBER=")
b=int(input("ENTRE SECOND NUMBER=")

c=a+b
print("ADDITION OF FIRST AND SECOND NUMBER IS =",c)

c=a-b
print("SUBSTRATION OF FIRST AND SECOND NUMBER IS =",c)

c=a*b
print("MULTIPLICATION OF FIRST AND SECOND NUMBER IS =",c)

c=a/b
print("DIVISION OF FIRST AND SECOND NUMBER IS =",c)

c=a%b
print("MODULOUS OF FIRST AND SECOND NUMBER IS =",c)


